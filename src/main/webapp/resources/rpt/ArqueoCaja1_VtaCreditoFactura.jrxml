	<queryString>
		<![CDATA[select 
        f.factura_id,
        f.nro_factura,
        f.fecha_emision,
        f.tipo_factura,
        ep.descripcion,
        f.forma_pago,
        c.nombre,
        fd.sub_total,
        (select sum(fd.sub_total) from facturas_detalle fd where f.forma_pago ='EF') as efectivo,
        (select sum(fd.sub_total) from facturas_detalle fd where f.forma_pago ='CQ') as cheque
from 
        facturas f inner join facturas_detalle fd on f.factura_id = fd.factura_id
        inner join cliente c on f.cliente_id = c.cliente_id
        inner join estado_impresion ep on f.estado_impresion = ep.estado_id]]>
	</queryString>
