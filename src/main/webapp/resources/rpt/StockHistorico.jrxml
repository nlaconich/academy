	<queryString>
		<![CDATA[SELECT
     Stock.stock_id AS Stock_ID,
     Stock.id_producto AS ID_Producto,
     Stock.observacion AS Observacion,
     Usuarios.username As Usuario,
     Producto.codigo_interno AS CodigoInterno,
     Producto.stock As STOCK,
     Producto.codigo As Codigo,
     Producto.descripcion As Descripcion,
     Stock.ingreso AS Ingreso,
     Stock.egreso AS Egreso,
     Stock.tipo_referencia As Referencia,
   ( SUM(Stock.ingreso) - SUM(stock.egreso) )as Suma
    
   
FROM
    "stock"Stock 
INNER JOIN "usuarios"Usuarios on Stock.usuario = Usuarios.usuario_id
INNER JOIN "producto"Producto on Stock.id_producto = Producto.producto_id 
group by
  Stock.stock_id,
  Stock.id_producto,
  Stock.observacion,
  Usuarios.username,
  Producto.codigo_interno,
  Producto.stock,
  Producto.codigo,
  Producto.descripcion,
  --Stock.ingreso,
  --Stock.egreso,
  Stock.tipo_referencia]]>
	</queryString>
