	<queryString>
		<![CDATA[select p.descripcion,
    pa.color,
    p.codigo_interno,
    pa.tipo,
    pa.textura,
    sum(pd.peso_actual) as total_pintura,
    p.unidad_medida,
    count(pd.producto_id) as cantidad_productos_stock

from productos_detalle as pd
    inner join producto as p ON (pd.producto_id = p.producto_id)
    inner join paleta as pa ON (p.paleta_id = pa.paleta_id)
WHERE pa.color != '---'  and pd.vigencia=true and pd.anulado=false
group by
p.descripcion,
    pa.color,
    p.codigo_interno,
    pa.tipo,pa.textura,p.unidad_medida
order by  pa.color;]]>
	</queryString>
