	<queryString>
		<![CDATA[Select
presupuesto."nro_presupuesto" AS presupuesto_nro_presupuesto,
presupuesto."fecha_emision" AS presupuesto_fecha_emision,
presupuesto."orden" AS presupuesto_orden,
cliente."nombre" AS cliente_nombre,
servicios."servicio" As servicios_descripcion,
SUM (presupuesto_detalle.precio_unitario) As Preciounitario_total,
SUM (presupuesto_detalle.sub_total) As Presupuesto_total

FROM
"public"."presupuesto" presupuesto
Inner Join "cliente" Cliente on  (presupuesto.cliente_id = Cliente.cliente_id)
Inner Join "servicios" servicios on (presupuesto.presupuesto_id = servicios.servicio_id)
Inner Join "presupuesto_detalle" presupuesto_detalle on  (presupuesto.presupuesto_id = presupuesto_detalle.presupuesto_detalle_id)
GROUP BY presupuesto.nro_presupuesto ,
   presupuesto.fecha_emision ,
   presupuesto.orden ,
   servicios.servicio ,
   cliente.nombre

ORDER BY
   cliente.nombre]]>
	</queryString>
