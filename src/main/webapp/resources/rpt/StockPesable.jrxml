	<queryString>
		<![CDATA[SELECT
     stock_pesable."peso_actual" AS stock_pesable_peso_actual,
     stock_pesable."fecha_registro" AS stock_pesable_fecha_registro,
     stock_pesable."usuario" AS stock_pesable_usuario,
     stock_pesable."id_producto" AS stock_pesable_id_producto,
     stock_pesable."id_paleta" AS stock_pesable_id_paleta,
     stock_pesable."observacion" AS stock_pesable_observacion,
     usuarios."username" AS usuarios_username,
     producto."descripcion" AS nombre_producto,
     paleta."color" AS color
FROM
     "public"."stock_pesable" stock_pesable INNER JOIN "public"."usuarios" usuarios ON stock_pesable."usuario" = usuarios."usuario_id"
     INNER JOIN "public"."producto" producto ON stock_pesable."id_producto" = producto."producto_id"
     INNER JOIN "public"."paleta" paleta ON stock_pesable."id_paleta" = paleta."paleta_id"
     INNER JOIN "public"."tipo_producto" tipo_producto ON producto."tipo_producto_id" = tipo_producto."tipo_id"
WHERE
     tipo_producto."tipo_id" = 2]]>
	</queryString>
