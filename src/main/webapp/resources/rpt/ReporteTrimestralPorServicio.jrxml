	<queryString language="SQL">
		<![CDATA[SELECT
    
    Servicios.servicio_id AS SercicioId,
    Servicios.servicio AS Servicos,
    Servicios.precio_unitario As Precio_Unitario,
    Servicios.porcentual AS Porcentual,
    Servicios.medida_id As MedidaID,
    Servicios.producto_id As ProductoID,
    Medidas.medida_id AS MedidasID,
    Medidas.descripcion AS Descripcion,
    Medidas.representacion AS Unidad_de_Medida
    
 FROM
     "servicios" Servicios  
INNER JOIN "medidas" Medidas ON  Medidas.medida_id = Servicios.medida_id
  WHERE 
Servicios.servicio_id = 1]]>
	</queryString>
