	<queryString>
		<![CDATA[select
cliente.nombre as cliente,
ot.orden_nro as otnro,
ot.fecha_inicio as inicio,
ot.fecha_fin as fin,
a.descripcion as articulo,
color.color as color,
ot.anticipo as anticipo,
p.descripcion,
otd.cantidad as cantidad,
servi.servicio as servicio,
otd.cabina as cabina,
otd.granallado as habilitar_granallado,
otd.hornogrande as habilitar_horno,
otd.pretratado as habilitar_pretratado

from
orden_trabajo_detalle otd

left join ordenes_trabajo ot on otd.orden_id=ot.orden_id

inner join cliente cliente on ot.cliente_id=cliente.cliente_id
left join prioridad p on ot.prioridad_id=p.prioridad_id
left join servicios servi on otd.servicio_id=servi.servicio_id
left join paleta color on otd.paleta_id=color.paleta_id
left join articulos a on otd.articulo_id=a.articulo_id
WHERE   ot.fecha_inicio::date between $P{fecha_Desde} and $P{fecha_Hasta} and ot.estado_id != 10
order by ot.fecha_inicio, ot.orden_nro]]>
	</queryString>
