	<parameter name="FECHA_HASTA" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
Presupuesto.nro_presupuesto ,
Presupuesto.orden,
Presupuesto.fecha_emision,
Presupuesto.fecha_vencimiento,
Presupuesto.tipo_presupuesto,
Presupuesto.forma_pago,
SUM(PD.sub_total) as total
FROM
 "presupuesto" Presupuesto inner join "presupuesto_detalle" PD on Presupuesto.presupuesto_id = PD.presupuesto_id

WHERE

    Presupuesto.fecha_emision::date = $P{FECHA_HASTA} and  Presupuesto.tipo_presupuesto='CREDITO'
GROUP BY
Presupuesto.nro_presupuesto ,
Presupuesto.orden,
Presupuesto.fecha_emision,
Presupuesto.fecha_vencimiento,
Presupuesto.tipo_presupuesto,
Presupuesto.forma_pago]]>
	</queryString>
