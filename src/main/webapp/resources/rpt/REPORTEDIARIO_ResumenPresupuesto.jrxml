	<parameter name="FECHA_HASTA" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
Presupuesto.nro_presupuesto ,
Cliente.nombre,
Presupuesto.orden,
Presupuesto.fecha_emision,
Presupuesto.fecha_vencimiento,
Presupuesto.tipo_presupuesto,
Presupuesto.forma_pago,
SUM(PD.sub_total) as total
FROM
 "presupuesto" Presupuesto
inner join "presupuesto_detalle" PD on Presupuesto.presupuesto_id = PD.presupuesto_id
Inner Join "cliente" Cliente on  (Presupuesto.cliente_id = Cliente.cliente_id)

WHERE
    Presupuesto.fecha_emision::date = $P{FECHA_HASTA}
GROUP BY
Presupuesto.nro_presupuesto ,
Cliente.nombre,
Presupuesto.orden,
Presupuesto.fecha_emision,
Presupuesto.fecha_vencimiento,
Presupuesto.tipo_presupuesto,
Presupuesto.forma_pago
ORDER BY
 Presupuesto.nro_presupuesto]]>
	</queryString>
