	<queryString>
		<![CDATA[SELECT

    cajasGastos.descripcion AS Descripcion,
    cajasGastos.monto_egreso AS MontoEgreso,
    cajasGastos.monto_ingreso AS MontoIngreso,
    cajasGastos.monto_egreso_chq AS MontoEgresoChq,
    cajasGastos.monto_ingreso_chq AS MontoIngresoChq,
    cajasGastos.ingreso_bancos AS MontoIngresoBnco,
    cajasGastos.fecha AS Fecha,
     Usuario.username AS Usuario


 FROM
     "cajas_gastos" cajasGastos
inner join "usuarios"Usuario on cajasGastos.usuario_id = Usuario.usuario_id
WHERE
     cajasGastos.fecha::date = $P{fecha_Diaria}


ORDER BY
cajasGastos.descripcion]]>
	</queryString>
