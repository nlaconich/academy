	<parameter name="Cliente" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT

     Factura.nro_factura AS FacturaNro,
     Factura.tipo_factura AS TipoFactura,
     Factura.fecha_emision AS Fecha_Emision,
     Factura.tipo_factura AS TipoFactura,
     Factura.estado_financiero AS Estadofinanciero,
     Factura.tipo_factura AS TipoFactura,
     Factura.forma_pago AS FormaPago,
     Cliente.ruc AS Cliente_ruc,
     Cliente.telefono AS Cliente_telefono,
     Cliente.nombre AS Cliente_nombre,
     Cliente.ciudad AS Cliente_direccion


 FROM

 "facturas" Factura
 Inner Join "cliente" Cliente on Factura.cliente_id = Cliente.cliente_id
 Inner Join "facturas_detalle" Facturas_Detalle on Facturas_Detalle.factura_detalle_id = Facturas_Detalle.factura_detalle_id]]>
