	<queryString>
		<![CDATA[Select f."fecha_vencimiento" AS fechaVencimiento,
       f."nro_factura" AS nroFactura,
       fd."sub_total" AS subTotal,
       c."cliente_id" AS clienteId,
       c."nombre" AS clienteNombre,
(SELECT current_date - f.fecha_vencimiento as dias from facturas f where f.cliente_id=$P{cliente_id})
       
       
       
       
From facturas f INNER JOIN  facturas_detalle fd ON f."factura_id"=fd."factura_id"
