	<parameter name="FECHA_HASTA" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    cajasGastos.monto_ingreso AS MontoIngreso,
    cajasGastos.monto_egreso AS MontoEgreso,
    cajasGastos.monto_ingreso_chq AS MontoIngresoCheque,
    cajasGastos.fecha AS Fecha,
    cliente.nombre AS ClienteNombre,
    facturas.nro_factura AS Nro



 FROM
     "cajas_gastos" cajasGastos


inner join "facturas"Facturas on cajasGastos.gastos_id= Facturas.factura_id
inner join "cliente"Cliente on cajasGastos.gastos_id = Cliente.cliente_id
WHERE
     cajasGastos.fecha::date = $P{FECHA_HASTA}

GROUP BY Facturas.nro_factura ,
      cajasGastos.monto_ingreso,
      cajasGastos.monto_egreso,
      cajasGastos.monto_ingreso_chq,
      cajasGastos.fecha,
      Cliente.nombre

ORDER BY
   Cliente.nombre]]>
	</queryString>
