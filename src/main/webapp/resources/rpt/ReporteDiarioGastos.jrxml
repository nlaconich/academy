	<parameter name="fecha_Hasta" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT f.tipo_factura,
sum(sub_total) as total_venta 
FROM facturas as f
inner join facturas_detalle as fd ON (f.factura_id = fd.factura_id)
WHERE f.fecha_emision::date = $P{fecha_Hasta}
group by f.tipo_factura

SELECT p.tipo_presupuesto,
sum(pd.sub_total) as total_venta 
FROM presupuesto as p
inner join presupuesto_detalle as pd ON (p.presupuesto_id = pd.presupuesto_id)
WHERE p.fecha_emision::date = $P{fecha_Hasta}
group by p.tipo_presupuesto

SELECT 
case (f.tipo_factura)
WHEN 'CONTADO' 
THEN sum(sub_total) END as CONTADO,
case (f.tipo_factura)
WHEN 'CREDITO'
THEN
sum(sub_total) 
END as CREDITO 
FROM facturas as f
inner join facturas_detalle as fd ON (f.factura_id = fd.factura_id)
WHERE f.fecha_emision::date = $P{fecha_Hasta}
group by f.tipo_factura]]>
	</queryString>
